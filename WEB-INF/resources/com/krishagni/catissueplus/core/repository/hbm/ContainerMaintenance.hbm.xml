<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class name="com.krishagni.catissueplus.core.administrative.events.ContainerMaintenance" table="OS_CONTAINER_MAINTENANCE" batch-size="25">
    <id name="id" column="IDENTIFIER" type="long" unsaved-value="null">
      <generator class="native">
        <param name="sequence">OS_CONTAINER_MAINTENANCE_SEQ</param>
      </generator>
    </id>

    <property name="lastMaintained" column="LAST_MAINTAINED"/>

    <many-to-one name="storageContainer" column="STORAGE_CONTAINER_ID" unique="true" cascade="save-update"/>
  </class>
</hibernate-mapping>
