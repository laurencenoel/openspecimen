<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog 
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">


 <changeSet author="vpawar" id="F infertility disorder PVs">
    <preConditions onFail="MARK_RAN">
      <sqlCheck expectedResult="0">
        select 
          case when (count(*) > 0) then 1 else 0 end 
        from 
          catissue_permissible_value
        where
          public_id = 'f_infertility_disorder'
      </sqlCheck>
    </preConditions>
    <loadUpdateData file="db/7.0/permissible-values/f-infertility-disorder.csv" tableName="catissue_permissible_value" primaryKey="identifier">
      <column name="identifier" type="COMPUTED"/>
    </loadUpdateData>
  </changeSet>
  
